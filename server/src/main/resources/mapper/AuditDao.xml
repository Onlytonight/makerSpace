<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qks.makerSpace.dao.AuditDao">

    <select id="getAuditById" resultType="com.qks.makerSpace.entity.Audit">
        select *
        from audit
        where audit_id = #{id}
    </select>

    <update id="changeAuditById" parameterType="Map">
        update audit
        set audit_time = #{auditTime}, is_audit = #{isAudit}
        where enterprise_id = #{enterpriseId}
    </update>

</mapper>